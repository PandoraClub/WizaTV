<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<default type="button">
		<width>125</width>
		<height>40</height>
		<font>regular</font>
		<textcolor>Color1</textcolor>
		<focusedcolor>Focused</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<selectedcolor>Selected</selectedcolor>
		<texturenofocus border="2" colordiffuse="Color3">views/button.png</texturenofocus>
		<texturefocus colordiffuse="Color1">colorize/100.png</texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="edit">
		<label>-</label>
		<hinttext>-</hinttext>
		<textcolor>Color1</textcolor>
		<disabledcolor>Disabled</disabledcolor>
        <invalidcolor>Warning</invalidcolor>
		<pulseonselect>false</pulseonselect>
		<texturefocus>-</texturefocus>
        <texturenofocus>-</texturenofocus>
	</default>

	<default type="fadelabel">
		<font>regular</font>
		<height>27</height>
		<aligny>center</aligny>
		<textcolor>Color1</textcolor>
		<focusedcolor>Focused</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<scroll>false</scroll>
		<scrollout>true</scrollout>
		<pauseatend>200</pauseatend>
		<resetonlabelchange>true</resetonlabelchange>
	</default>

	<default type="label">
		<font>regular</font>
		<height>27</height>
		<aligny>center</aligny>
		<textcolor>Color1</textcolor>
		<disabledcolor>Disabled</disabledcolor>
		<scroll>false</scroll>
		<scrollspeed>autoscroll_Speed</scrollspeed>
		<scrollsuffix> | </scrollsuffix>
	</default>

	<default type="multiimage">
		<include>screen_Dimensions</include>
		<aspectratio align="center">scale</aspectratio>
		<randomize>true</randomize>
		<loop>yes</loop>
		<timeperimage>slideshow_Timer</timeperimage>
		<fadetime>fadetime_Slideshow</fadetime>
	</default>

	<default type="progress">
		<texturebg border="0" colordiffuse="Color4">dialogs/progress/bg.png</texturebg>
		<lefttexture border="0" colordiffuse="Color4">colorize/100.png</lefttexture>
		<midtexture border="0" colordiffuse="Color4">colorize/100.png</midtexture>
		<righttexture border="0" colordiffuse="Color4">colorize/100.png</righttexture>
		<overlaytexture>-</overlaytexture>
	</default>
	
	<default type="radiobutton">
		<font>regular</font>
		<textureradioonnofocus>dialogs/radio/on-nf.png</textureradioonnofocus>
		<textureradiooffnofocus>dialogs/radio/off-nf.png</textureradiooffnofocus>
		<textureradioonfocus>dialogs/radio/on-fo.png</textureradioonfocus>
		<textureradioofffocus>dialogs/radio/off-fo.png</textureradioofffocus>
		<textureradioondisabled colordiffuse="Color3">dialogs/radio/on-fo.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="Color3">dialogs/radio/off-fo.png</textureradiooffdisabled>
		<pulseonselect>false</pulseonselect>
		<radioposx></radioposx>
		<radioposy></radioposy>
	</default>

	<default type="scrollbar">
		<width>8</width>
		<showonepage>false</showonepage>
		<texturesliderbackground>views/pagecontrol/background.png</texturesliderbackground>		
		<texturesliderbar>views/pagecontrol/grip-nf.png</texturesliderbar>
		<texturesliderbarfocus>views/pagecontrol/grip-fo.png</texturesliderbarfocus>
		<textureslidernib>-</textureslidernib>
		<textureslidernibfocus>-</textureslidernibfocus>
		<include>fadeinout</include>
		<colordiffuse>Color1</colordiffuse>
	</default>

	<default type="sliderex">
		<height>40</height>
		<texturefocus border="3">colorize/70.png</texturefocus>
		<texturenofocus border="3">views/nofocus.png</texturenofocus>
		<sliderwidth>100</sliderwidth>
		<sliderheight>20</sliderheight>
		<texturesliderbar>dialogs/slider/bar.png</texturesliderbar>
		<textureslidernib>dialogs/slider/nib-nf.png</textureslidernib>
		<textureslidernibfocus>dialogs/slider/nib-fo.png</textureslidernibfocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrol">
		<height>40</height>
		<font>regular</font>
		<textcolor>Color4</textcolor>
		<focusedcolor>Color1</focusedcolor>
		<textoffsetx>15</textoffsetx>
		<textoffsety>-2</textoffsety>
		<reverse>yes</reverse>
		<aligny>center</aligny>
		<pulseonselect>false</pulseonselect>
		<textureup>dialogs/spin/up-nf.png</textureup>
		<texturedown>dialogs/spin/down-nf.png</texturedown>
		<textureupfocus>dialogs/spin/up-fo.png</textureupfocus>
		<texturedownfocus>dialogs/spin/down-fo.png</texturedownfocus>
		<textureupdisabled colordiffuse="Color3">dialogs/spin/up-nf.png</textureupdisabled>
		<texturedowndisabled colordiffuse="Color3">dialogs/spin/down-nf.png</texturedowndisabled>
		<spinwidth>26</spinwidth>
		<spinheight>26</spinheight>
	</default>
	
	<default type="spincontrolex">
		<height>40</height>
		<font>regular</font>
		<textcolor>Color1</textcolor>
		<focusedcolor>Focused</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<textoffsety>0</textoffsety>
		<textoffsetx>15</textoffsetx>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
		<texturefocus border="2">colorize/70.png</texturefocus>
		<texturenofocus border="2">views/nofocus.png</texturenofocus>
		<textureup colordiffuse="Color3">settings/spin/up-nf.png</textureup>
		<textureupfocus colordiffuse="Color5">settings/spin/up-fo.png</textureupfocus>
		<texturedown colordiffuse="Color3">settings/spin/down-nf.png</texturedown>
		<texturedownfocus colordiffuse="Color5">settings/spin/down-fo.png</texturedownfocus>
		<textureupdisabled colordiffuse="Color3">dialogs/spin/up-nf.png</textureupdisabled>
		<texturedowndisabled colordiffuse="Color3">dialogs/spin/down-nf.png</texturedowndisabled>
		<spinwidth>26</spinwidth>
		<spinheight>26</spinheight>
	</default>

	<default type="textbox">
		<align>left</align>
		<font>regular</font>
		<height>auto</height>
		<aligny>top</aligny>
		<textcolor>Color3</textcolor>
	</default>

	<default type="togglebutton">
		<font>regular</font>
		<textcolor>Color1</textcolor>
		<focusedcolor>Focused</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<texturenofocus>-</texturenofocus>
		<texturefocus>-</texturefocus>
		<alttexturenofocus>-</alttexturenofocus>
		<alttexturefocus>-</alttexturefocus>
		<pulseonselect>false</pulseonselect>
	</default>
</includes>
