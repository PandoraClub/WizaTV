<?xml version="1.0" encoding="UTF-8"?>
<window>
	<coordinates>
		<system>1</system>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<control type="group">
			<include>transition_Info</include>
			<visible>![Window.IsActive(startup) | Window.IsActive(fullscreenvideo) | Window.IsActive(infodialog) | Window.IsActive(busydialog)]</visible>
			<!--line 1-->
			<control type="label" id="40">
				<label>$INFO[Control.GetLabel(30),,: ]$INFO[Control.GetLabel(32),,%]</label>
				<include>toast_Top</include>
			</control>
			<!--line 2-->
			<control type="label" id="31">
				<include>toast_Bottom</include>
			</control>
			<control type="label" id="30">
				<visible>false</visible>
			</control>
			<control type="progress" id="32">
				<texturebg>-</texturebg>
				<lefttexture>-</lefttexture>
				<midtexture>-</midtexture>
				<righttexture>-</righttexture>
				<overlaytexture>-</overlaytexture>
				<visible>false</visible>
			</control>
			<!--default-->
			<control type="image">
				<include>toast_Icon</include>
				<texture>toast/info.png</texture>
				<visible>!Player.Muted + IntegerGreaterThan(Player.Volume,0)</visible>
			</control>
		</control>
	</controls>
</window>