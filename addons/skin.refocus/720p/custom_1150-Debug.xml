<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1150">
	<visible>!Window.IsVisible(startup)</visible>
	<zorder>20</zorder>
	<controls>
		<!--grid-->
		<control type="group">
			<visible>Skin.HasSetting(dev.grid)</visible>
			<control type="image">
				<include>screen_Dimensions</include>
				<texture>overlays/debuggrid720.png</texture>
				<colordiffuse>Color3</colordiffuse>
			</control>
		</control>
		<!--info-->
		<control type="group">
			<visible>Skin.HasSetting(dev.overlay)</visible>	
			<control type="grouplist">
				<left>10</left>
				<top>80</top>
				<height>640</height>
				<itemgap>0</itemgap>
				<control type="label" id="1">
					<label>Container.ViewMode: $VAR[window:Debug]$INFO[Container.Viewmode, - ]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="2">
					<label>System.CurrentControl: $INFO[System.CurrentControl]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="3">
					<label>Container.FolderName: $INFO[Container.FolderName]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="4">
					<label>Container.FolderPath: $INFO[Container.FolderPath]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="5">
					<label>System.ScreenResolution: $INFO[System.ScreenResolution]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="6">
					<label>ListItem.FileNameAndPath: $INFO[ListItem.FileNameAndPath]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="7">
					<label>ListItem.FolderPath: $INFO[ListItem.FolderPath]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="8">
					<label>ListItem.VideoCodec: $INFO[ListItem.VideoCodec]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="9">
					<label>ListItem.VideoAspect: $INFO[ListItem.VideoAspect]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="10">
					<label>ListItem.VideoResolution: $INFO[ListItem.VideoResolution]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="11">
					<label>ListItem.AudioCodec: $INFO[ListItem.AudioCodec]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="12">
					<label>ListItem.AudioChannels: $INFO[ListItem.AudioChannels]</label>
					<include>debug_Label</include>
				</control>
				<control type="label" id="13">
					<label>ListItem.AudioLanguage: $INFO[ListItem.AudioLanguage]</label>
					<include>debug_Label</include>
				</control>
			</control>
		</control>
	</controls>
</window>